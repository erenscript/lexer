-- Scripts:

local function HMTFZTM_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.TextBox.Text == "LEXERBESTPHIECLEXTRANOOB" then
			script.parent.Parent.Parent.Frame.Visible = false
		end
	end)
end
coroutine.wrap(HMTFZTM_fake_script)()
